<h1>Editar trabajo/evaluacion</h1>
<%= form_for :work, url: works_path do |f| %>
  <div class="col-xs-4">
    <p><%= f.label :name, 'Nombre' %>
    <%= f.text_field :name, :class => 'form-control' %>
    <p><%= f.label :nota_total %>
    <%= f.text_field :nota_total, :class => 'form-control' %>
    <p><%= f.label :created_at, 'Fecha' %>
    <%= f.date_select :created_at %>
    <p><%= f.submit 'Editar', :class => 'btn btn-lg btn-success' %>
  </div>
<% end %>